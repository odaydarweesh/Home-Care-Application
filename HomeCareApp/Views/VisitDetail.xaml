<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HomeCareApp.Views.VisitDetail">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="User"
                  x:Name="User"
                  TextTransform="Uppercase"/>
            <Entry Placeholder="Patient"
                  x:Name="Patient"
                  TextTransform="Uppercase"/>
            <Entry Placeholder="Effort"
                  x:Name="Effort"
                  TextTransform="Uppercase"/>
            <Entry Placeholder="StartTime"
                  x:Name="StartTime"
                  TextTransform="Uppercase"/>
            <Entry Placeholder="EndTime"
                  x:Name="EndTime"
                  TextTransform="Uppercase"/>
            <Button Text="Save"
                    Clicked="Button_Clicked"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>