<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HomeCareApp.FlyoutPageMenu"
             xmlns:pages="clr-namespace:HomeCareApp" 
            xmlns:pages1="clr-namespace:HomeCareApp.Views">
  <FlyoutPage.Flyout>
    <pages:FlyoutPageMenuFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
                <pages1:HomePage />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>